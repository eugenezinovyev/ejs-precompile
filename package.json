{
  "name": "ejs-precompile",
  "description": "Precompile EJS templates to JS functions",
  "type": "module",
  "version": "1.0.0",
  "main": "dist/api.cjs",
  "module": "dist/api.mjs",
  "types": "dist/api.d.ts",
  "bin": {
    "ejs-precompile": "./dist/cli.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist && tsc",
    "build": "ts-node --esm esbuild.ts",
    "presamples": "npm run build",
    "samples": "node ./dist/cli.js -i ./samples -o ./samples -l typescript",
    "postsamples": "ts-node --esm ./samples/run-samples.ts",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest"
  },
  "keywords": [
    "ejs",
    "precompile",
    "compile",
    "template",
    "view"
  ],
  "author": "Eugene Zinovyev <eugene.zinovyev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "ejs": "^3.1.9",
    "esbuild-node-externals": "^1.8.0",
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@lcov-viewer/istanbul-report": "^1.3.0",
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.3",
    "@types/node": "^16.18.40",
    "esbuild": "^0.18.17",
    "jest": "^29.6.2",
    "nanoid": "^4.0.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
